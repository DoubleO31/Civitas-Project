{
  "name": "Civitas-app",
  "version": "1.0.0",
  "scripts": {
    "client": "cd client && npm start",
    "server": "nodemon server.js",
    "dev": "concurrently --kill-others-on-fail \"npm run server\" \"npm run client\"",
    "start": "node server.js",
    "heroku-postbuild": "cd client && npm install && npm install --only=dev --no-shrinkwrap && npm run build"
  },
  "dependencies": {
    "@google-cloud/storage": "^1.7.0",
    "bcrypt": "^3.0.0",
    "bcryptjs": "^2.4.3",
    "color-thief": "^2.2.3",
    "express": "^4.16.2",
    "file-type": "^8.1.0",
    "formidable": "^1.2.1",
    "jsonwebtoken": "^8.3.0",
    "mongoose": "^5.2.1",
    "muller": "^1.1.0",
    "multer": "^1.3.1",
    "nodemon": "^1.18.3",
    "npm": "^6.3.0",
    "passport": "^0.4.0",
    "passport-local": "^1.0.0",
    "prop-types": "^15.6.2",
    "validator": "^10.4.0"
  },
  "devDependencies": {
    "concurrently": "^3.5.0"
  }
}
